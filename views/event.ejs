        <% if(event.videoId){%>

            <div id="eventMap">
                <iframe width="560" height="315" src="https://www.youtube.com/embed/<%= event.videoId %>" title="YouTube video de
                player" frameborder="0"
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                    allowfullscreen></iframe>


                <% } else {%>
                    <div id="eventMap">
                        <iframe width="560" height="315"
                            src="https://www.youtube.com/embed?listType=playlist&list=RDCLAK5uy_k27uu-EtQ_b5U2r26DNDZOmNqGdccUIGQ"
                            title="YouTube video de
                  player" frameborder="0"
                            allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                            allowfullscreen></iframe>
                        <% } %>
                            <a href="https://www.google.com/maps/search/?api=1&query=<%= event.location %>"
                                target="_blank">
                                <img id="mapApi"
                                    src="https://maps.googleapis.com/maps/api/staticmap?center=<%= event.location %>&zoom=15&size=600x300&markers=color:red%7Clabel:A%7C<%= event.location %>&key=AIzaSyB4YGxGK5iXbhZLd1y01bPwy5vYLTwHO2k">
                            </a>


                    </div>
                    <div class="btns"> <span> Click the button</span> <button onclick="copyWebsiteLink()">Share
                            Link</button> 
                    </div>
                    <div id="eventContainer">
                        <h1>
                            <%= event.summary %>
                        </h1>
                        <h1>
                            <%= event.user %>
                        </h1>
                        <span><i class="fas fa-map-marker-alt"></i> Location:</span>
                        <input id="locationInput" type="text" value="<%= event.location %>" disabled>

                        <span><i class="far fa-calendar-alt"></i> Date:</span>
                        <input id="dateInput" type="text"
                            value="<%= new Date(event.start.dateTime).toLocaleDateString('en-US') %>" disabled>

                        <span><i class="far fa-clock"></i> Starts:</span>
                        <input id="startsInput" type="text"
                            value="<%= new Date(event.start.dateTime).toLocaleTimeString('en-US') %>" disabled>

                        <span><i class="far fa-clock"></i> Ends:</span>
                        <input id="endsInput" type="text"
                            value="<%= new Date(event.end.dateTime).toLocaleTimeString('en-US') %>" disabled>
                    </div>
                    <div class="btns">
                        <button onclick="toggleEditMode()"> Edit <i class="fas fa-pencil-alt"></i></button>
                        <button onclick="saveChanges()">Save</button>

                    </div>
                    <div>
                        <!-- TODO display messages here -->
                        <% if (messages?.length > 0) { %>
                            <div id="chat">
                            <% for (let n = 0; n < messages.length; n++) { %>
                                <div class="message">
                               <p class="speaker-name"><%=messages[n].createdBy %></p>
                                <p><%=messages[n].content%></p>
                                 <p><%=messages[n].createdAt %>t</p>
                                </div>
                            <% } %>
                           
                        <% } else { %>
                            <p> No messages for this event, be the first one to comment your thoughts below!</p>
                        <% } %>
                        <!-- CONFLICT multiple actions when clickin button -->
                        <!-- 1.- Form triggering POST to /message -->
                        <!-- 2.- msgBtn eventListener fetch /event/:eventId/message -->
                        <!-- <form action="/message" type="text" method="POST"> -->
                        <form class="form-container">
                            <input type="text" placeholder="Let's Talk" name="message">
                            <button class="msgBtn">messages</button>
                        </form>
                       

                        </div>


                        <!-- Contact Page Start -->

                    </div>



